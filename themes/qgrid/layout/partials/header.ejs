<section class="header">
	<nav class="nav">
		<a class="qgrid-logo"></a>
		<% if (!isIndex) { %>
			<span class="qgrid-search">
				<mat-form-field class="app-search">
					<input matInput
						   [(ngModel)]="search"
						   (ngModelChange)="onSearchChange()"
						   placeholder="Search"
						   autocomplete="off"
						   class="search"
						   id="search"
						   type="search" />
				</mat-form-field>
			</span>
		<% } %>
		<ul class="frameworks">
			<li class="ready">
				<a href="<%- url_for('guide/highlights.html') %>" target="_blank">Documentation</a>
			</li>
			<li class="ready">
				<a href="<%- url_for('guide/examples.html') %>">Examples</a>
			</li>
			<li class="ready">
				<a href="<%- url_for('https://github.com/qgrid/qgrid.github.io.git') %>">GitHub</a>
			</li>
		</ul>
		<a class="button" href="#">Become maintainer</a>
	</nav>
</section>