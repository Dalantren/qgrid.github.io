<nav>
    <ul>
        <% var start = site.pages.filter(function (p) {
            return p.group === 'Getting started'
        }) %>
        <% var api = site.pages.filter(function (p) {
            return p.group === 'API'
        }) %>

        <% if (start.length > 0) { %>
        <li><h3>Getting started</h3></li>
        <% start.each(function (p) { %>
        <% var fileName = p.path.replace(/^.+?\/([\w-]+)\.html/, '$1') %>
        <li>
            <a href="<%- url_for(p.path) %>"
               class="<%- page.title === p.title ? 'current' : '' %> "><%- p.title %></a>
        </li>
        <% })} %>
    </ul>
</nav>
